import random.randint
count = input("how many questions do you need")
count global

def letter():
	asd = random.randint(0,3)
	if asd = 0:
		return "a"
	elif asd = 1:
		return "b"
	elif asd = 2:
		return "c"
	elif asd = 3:
		return "d"


def randomize():
	for i in range(count):
		letter = letter()
		answer = (str(i)+letter+".")
		print()
		print()
		print(str(i)+"a.")
		print(str(i)+"b.")
		print(str(i)+"c.")
		print(str(i)+"d.")

randomize()
